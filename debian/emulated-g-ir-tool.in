set -eu

DEB_HOST_GNU_TYPE='@DEB_HOST_GNU_TYPE@'
TOOL_PATH='@TOOL_PATH@'

if command -v "${DEB_HOST_GNU_TYPE}-cross-exe-wrapper" > /dev/null; then
    exec "${DEB_HOST_GNU_TYPE}-cross-exe-wrapper" "${TOOL_PATH}" "$@"
else
    exec "${TOOL_PATH}" "$@"
fi
